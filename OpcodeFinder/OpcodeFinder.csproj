      <PrivateAssets>all</PrivateAssets>
    </PackageReference>
    <PackageReference Include="Iced" Version="1.17.0" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
  </ItemGroup>

</Project>
